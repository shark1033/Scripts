
#написать скрипт, который создаст дирректории для нескольких годов (2010-17),
#в них поддиректории для месяцев (01-12) и в каждой из них файлы (001-005.тхт)

#!/bin/bash
mkdir test
cd ./test
for i in {2010..2017}
	do
		mkdir $i
		cd ./$i
		for y in {01..12}
			do
				mkdir $y
				cd ./$y
				for x in {001..005}
					do
						touch $x.txt 
					done
				cd ../  #возврат к предыдущей папке
			done
		cd ../ #возврат к предыдущей папке
	done
ls -l
